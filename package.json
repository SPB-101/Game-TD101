{
  "name": "game-td101",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm-run-all --print-label build:dev:client watch:server",
    "watch:server": "nodemon",
    "build:dev:client": "npm run set-env:dev && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --node-env development --config webpack/webpack.client.ts",
    "build:dev:server": "npm run set-env:dev && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --node-env development --config webpack/webpack.server.ts",
    "build:prod": "npm-run-all --print-label clean build:prod:*",
    "build:prod:client": "npm run set-env:prod && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --node-env production --config webpack/webpack.client.ts",
    "build:prod:server": "npm run set-env:prod && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --node-env production --config webpack/webpack.server.ts",
    "start:server": "node dist/server.js",
    "set-env:dev": "cross-env NODE_ENV=development",
    "set-env:prod": "cross-env NODE_ENV=production",
    "lint": "npm-run-all lint:*",
    "lint:style": "npx stylelint './**/*.{css,scss}'",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "fix": "npm-run-all fix:lint:*",
    "fix:lint:src": "npx eslint --fix --ext=.ts,.tsx ./",
    "fix:lint:style": "npx stylelint './**/*.{css,scss}' --fix",
    "deploy": "heroku login && heroku container:login && heroku container:push web --app=yandex-praktikum-td && heroku container:release web --app=yandex-praktikum-td",
    "ssl": "sh scripts/ssl.sh",
    "hosts": "sh scripts/hosts.sh",
    "test": "jest",
    "test:update": "npm test -- -u",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SPB-101/game-td101.git"
  },
  "author": "Team SPB-101",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SPB-101/game-td101/issues"
  },
  "homepage": "https://github.com/SPB-101/game-td101#readme",
